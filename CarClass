package carresults;

/**
 *
 * @author zack
 */
class Car {
    public int yearModel;
public String make;
public int speed;

public Car()
{
yearModel=0;
speed=0;
make="2008 Subaru WRX";
}

public Car(int yearModel, String make, int speed)
{
this.yearModel=yearModel;
this.make=make;
}

public int getYear()
{
return yearModel;
}

public String getMake()
{
return make;
}

public int getSpeed()
{
return speed;
}

public int accelerate()
{
speed=speed+5;
return speed;
}

public int brake()
{
speed=speed-5;
return speed;
}

public void setYear(int yearValue)
{
this.yearModel=yearValue;
}

public void setMake(String make)
{
this.make=make;
}
